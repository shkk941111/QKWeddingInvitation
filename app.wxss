/**app.wxss**/
page{
  font-family: Arial, Helvetica, sans-serif;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  box-sizing: border-box;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.some-backgroud{
  background: white;
}
.some-text{
  color: black;
}
@media(prefers-color-scheme:dark){
  .some-backgroud{
    background: #1b1b1b;
  }
  .some-text{
    color: #ffffff;
  }
}

